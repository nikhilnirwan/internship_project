{
  "name": "root",
  "private": true,
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/register": "^7.14.5",
    "add": "^2.0.6",
    "lerna": "^4.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "scripts": {
    "lint": "lerna run lint --stream",
    "bm": "git pull && yarn install && yarn bootstrap && yarn build && pm2 restart televisit",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean --stream",
    "force-build": "yarn clean && yarn build",
    "build": "lerna run build --stream --concurrency 1",
    "build-admin": "lerna run --scope admin build --stream",
    "build-client": "lerna run --scope client  build --stream",
    "build-server": "lerna run --scope server build --stream",
    "format": "lerna run format --stream",
    "force-dev": "yarn clean && yarn dev",
    "dev": "lerna run dev --stream",
    "dev-server": "lerna run --scope server dev --stream",
    "dev-admin": "lerna run --scope admin --scope server dev --stream",
    "dev-client": "lerna run --scope client --scope server dev --stream",
    "start": "lerna run start --parallel",
    "cypress:open": "cypress open",
    "serve": "webpack serve --config config/webpack.dev.js --progress",
    "make:migration": "sequelize migration:generate --name",
    "migrate": "sequelize db:migrate",
    "migrate:rollback": "sequelize db:migrate:undo",
    "make:seed": "sequelize seed:generate --name",
    "seed": "sequelize db:seed:all",
    "seed:rollback": "sequelize db:seed:undo"
  },
  "dependencies": {
    "@atlaskit/theme": "^11.4.1",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^5.3.2",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-schedule": "^2.0.0",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "react-dom": "^17.0.2",
    "react-gesture-responder": "^2.1.0",
    "react-grid-dnd": "^2.1.2",
    "react-highlight-words": "^0.17.0",
    "react-list-drag-and-drop": "^0.9.2",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.55.1",
    "styled-components": "^5.3.0"
  }
}
